{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "inlineSources": true,
    "preserveConstEnums": true,
    "noEmitOnError": false,
    "removeComments": false,
    "downlevelIteration": false
  }
}
